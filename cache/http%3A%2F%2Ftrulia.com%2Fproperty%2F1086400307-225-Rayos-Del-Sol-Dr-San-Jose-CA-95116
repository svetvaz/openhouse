<script type="text/javascript">

trulia.ready(function() {
   // Save home prompt should only be shown once per user session.
  if (trulia.prompt_manager && !trulia.prompt_manager.isPromptCompleted('Overlay-CTA-Prompt')) {
    trulia.prompt_manager.completedPrompt('Overlay-CTA-Prompt');
  }

  var promptCfgObj = {
    closeTargetRole: $('[data-role="save_home_traceback"]'),
    trackingId: 'SaveHomePopup',
    closeEventString: 'followAction.followHome alreadyFollowed.followHome'
  };

  var pdpOverlayPrompt = Backbone.View.extend({

    stickyCfgObj: {},

    trackingObjValues: {
      IMPRESSION:  'Impression',
      CLICK_ACTION: 'ClickCTA',
      CLICK_CLOSE:  'Close'
    },

    defaults:
    {
      closeTargetEl: false,
      doShow: function(element) { $(element).delay(1000).animate({opacity:1}); }
    },

    events: function ()
    {
      var self = this;

      this.$el.on('scrolly:atSetPoint', this.togglePrompt);

      this.$('[data-role=click_cta]').click(function ()
      {
        self.fireTracking(self.trackingObjValues.CLICK_ACTION);
      });

      this.listenTo(trulia.LegacyEventWrapper($(document)), this.options.closeEventTrigger, this.traceBackClose);

      return {
        'click .boxClose': 'closeBox'
      }
    },

    initialize: function ()
    {
      _.bindAll(
        this,
        'togglePrompt',
        'removePrompt',
        'traceBackClose',
        'fadeClose',
        'closeBox'
      );

      this.options = _.extend(this.defaults, this.options);
      this.stickyCfgObj = this.options.stickyCfgObj;

      // Initialize actual sticky element here
      this.$el.sticky(this.stickyCfgObj);

    },

    togglePrompt: function (event, scrollDown)
    {
      if (scrollDown)
      {
        this.options.doShow(this.$el);
        this.fireTracking(this.trackingObjValues.IMPRESSION);
      }
    },

    closeBox: function ()
    {
      this.fireTracking(this.trackingObjValues.CLICK_CLOSE);
      this.traceBackClose();
    },

    traceBackClose: function ()
    {
      var element = this.$el
        , returnTargetEl = this.options.closeTargetEl
        ;

      // If there's no target element defined, skip this and do a normal fadeClose instead
      if (returnTargetEl == false || returnTargetEl.length == 0)
      {
        this.fadeClose();
      }
      else
      {
        var targetElProperties = {
          width: returnTargetEl.outerWidth(),
          height: returnTargetEl.outerHeight(),
          margin: 0,
          left: returnTargetEl.position().left - 2
        }

        if (element.css('bottom') !== 'auto')
        {
          targetElProperties['bottom'] = $(window).height() - returnTargetEl.outerHeight() - returnTargetEl.position().top - 2;
        }
        else
        {
          targetElProperties['top'] = returnTargetEl.position().top;
        }

        $('.boxClose').closest('.box').fadeOut('slow');
        element.css({'border':'2px solid #000', 'border-radius':'4px', 'height': element.height() + 'px'});
        element.delay(500).animate(targetElProperties, 850, 'swing').fadeOut('500', this.removePrompt);
      }
    },

    fadeClose: function ()
    {
      this.$el.fadeOut('500', this.removePrompt);
    },

    removePrompt: function ()
    {
      this.$el.remove();
    },

    fireTracking: function(trackingAction)
    {
      o_track_ql_click(this.options.trackingId + '|' + trackingAction);
    }

  });

  if ($('#overlayPromptLeft').length)
  {
    var overlayLeft = new pdpOverlayPrompt({
      el: '#overlayPromptLeft',
      closeTargetEl: promptCfgObj.closeTargetRole,
      trackingId: promptCfgObj.trackingId,
      closeEventTrigger: promptCfgObj.closeEventString,

      stickyCfgObj: {
        css: { opacity:0 },
        toggleClass: 'hideVisually',
        targetEl: $('#photoMapContainer')
      },

      doShow: function(element)
      {
        $(element).delay(5000).animate({opacity:1});
      }

    });
  }

});
</script>
      
  
  
