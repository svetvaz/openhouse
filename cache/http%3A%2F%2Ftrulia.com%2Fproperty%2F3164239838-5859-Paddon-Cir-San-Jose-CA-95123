<script>
  $(document).ready(function () {
    var property = trulia.propertyData.get();
    var photoPlayerConfig = {
      'showThumbnails' : true ,
      'size' : {'width' : '640px', 'height' : 'auto'},
      'maxSize' : {'max-width' : '640px', 'max-height' : '480px'},
      'thumbnailMaxSize' : {'max-width' : '70px', 'max-height' : '50px'},
      'currentCountSelector' : '.photoPlayerCounterCurrent',
      'maxCountSelector' : '.photoPlayerCounterMax'
    };
    var floorPlanPlayerConfig = {
      'showThumbnails' : false ,
      'size' : {'width' : '640px', 'height' : 'auto'},
      'maxSize' : {'max-width' : '640px', 'max-height' : '480px'},
      'thumbnailMaxSize' : {'max-width' : '70px', 'max-height' : '50px'},
      'currentCountSelector' : '.floorplanCounterCurrent',
      'maxCountSelector' : '.floorplanCounterMax'
    },
      isFloorPlanLoaded = false;

    

    $('#mediaTabs').truliaTabs();

          
                      $('#photoPlayer').truliaPhotoPlayer(photoPlayerConfig);
      
          
    $('#mediaTabsContents').on('tab:show', function (e, tab, index) {
      if (!$(tab).hasClass('floorplanTabContent'))
      {
        return;
      }

      if (!isFloorPlanLoaded)
      {
        $(this).trigger('loadFloorplanPlayer');
        // by default we update the floorplan url hash the 1
        trulia.pdp.action_handlers.updateHash('floorplan', 1);
      }
    });

    $('#mediaTabsContents').on('loadFloorplanPlayer', function() {
      if (!isFloorPlanLoaded)
      {
        $('.floorplanPhotoPlayer').truliaPhotoPlayer(floorPlanPlayerConfig);
        isFloorPlanLoaded = true;
      }
    });

    if ($('#mediaTabsContents .floorplanTabContent').hasClass('active'))
    {
      $('#mediaTabsContents').trigger('loadFloorplanPlayer');
    }

    
    
  	$('span.timeago').timeago();
    $('.toggle').truliaToggle();
    $('.rating').rating();
    $('.oh_details, #listingDefinition').tooltip();

    $('#more_actions').tooltip({
      anchor: 'bottom',
      trigger: 'manual',
      selector: '#more_action_list',
      delay: 0
    });

    $('.taxInfoAssess').each(function(){
    	$(this).tooltip({
        anchor: 'bottom',
        offset: '-10x0',
        selector: '#taxTipAssessment' + $(this).data('year')
      });
    });

    $('.taxInfoMarket').each(function(){
      $(this).tooltip({
        anchor: 'bottom',
        offset: '-10x0',
        selector: '#taxTipMarket' + $(this).data('year')
      });
    });

    $('.pdpHeaderTooltip span').each(function(){
      $(this).tooltip();
    });

    $('#property_bar').sticky({
      'targetEl': $('#mediaTabs'),
      'toggleClass': 'hideVisually'
    });

    $('#boardwalkFixedAskAgentForm').sticky({
      'targetEl': $('#mediaTabs'),
      'toggleClass': 'hideVisually'
    });

    $('#foreclosure_types').truliaTabs({
      selected: 0
    });

    $('#local_info_tabs').truliaTabs();


    trulia.pdp.rateAndReview.init();


        trulia.pdp.localInfo.init();
    trulia.pdp.action_handlers.init();

    
    
    
    
    $('.pinit').attr('href', 'http://pinterest.com/pin/create/button/?url='+document.location.toString().replace(document.location.hash, '')+'&media=http://thumbs.trulia-cdn.com/pictures/thumbs_5/ps.70/d/4/e/8/picture-uh=33213fa1fa20a13c9826ab32b5dfde6b-ps=d4e8a3fb37f845b1db1f4246cb1c9f5-5859-Paddon-Cir-San-Jose-CA-95123.jpg'+'&description='+escape(document.title));

    if (window.location.hash)
    {
      switch (window.location.hash)
      {
        case '#claim':
          location.href = _SITE_ROOT + "claim/agent/" + trulia.propertyData.get().id;
          break;
        case '#contact':
          trulia.ContactUser.showContactAgentPopup({}, 'details', 'LeadContactAgent');
          break;
        case '#sharebox':
          reset_email_box('property');
          show_pos_menu(event, 'send_property_to_friend_email_popup');
          o_track_ql_click('AutoLoad|Share_Link');
          break;
      }

      // Check url for floorplan/photo hash and go to that specific image
      var match = window.location.hash.match(/#(floorplan|photo)-([0-9]+)/);

      if (match && match.length === 3)
      {
        if (match[1] == 'floorplan')
        {
          $('#mediaTabs .floorplans').click();
          $('#floorplanPhotoPlayer').truliaPhotoPlayer('selectItem', (match[2] - 1));
        }
        else
        {
          // The default photo player uses image tags to display property photos and the large
          // photo player uses background images so updating the image is handled differently
          var $photoPlayer = $('#photoPlayer');
          if ($photoPlayer.length > 0)
          {
            $photoPlayer.truliaPhotoPlayer('selectItem', (match[2] - 1) );
          }
          else
          {
            var $photoPlayerSlideShow = $('#photoPlayerSlideshow');
            if ($photoPlayerSlideShow.length > 0)
            {
              // Update the photo on the large photo player
              $photoPlayerSlideShow.truliaPhotoPlayer('selectItem', (match[2] - 1));
              // Update the photo on the static photo player
              trulia.pdp.action_handlers.updateStaticPhotoPlayerImage($photoPlayerSlideShow, (match[2] - 1));
            }
          }
        }
      }
    }

        
    // code/ajax to prepare the lead recommender bypass lazy loader should only run if we've injected the markup for it into the lead forms
    var $lead_recommender_bypass_container = $('.lead_recommender_bypass_container');

    if ($lead_recommender_bypass_container.length)
    {
      // ajax call to get the chechbox and the modal for the lead recommender bypass
      var lrmBypassAjax = $.ajax({
        url: site_root + '_ajax/PDP/PDP/lrm_bypass/',
        dataType: 'json',
        type: 'get',
        data: {
          property_id: '3164239838',
          property_status: 'For Sale'
        }
      });

      // this function handles the ajax response, but only runs after coreg is done
      // modifying its forms (see code immediately below this function)
      var lrmBypassAjaxResponseHandler = function (data) {
        if (data && parseInt(data['bypass_property_count']) && data['markup'])
        {
          // inject the bypass elements
          $lead_recommender_bypass_container.html(data['markup']);

          // prepare the bypass elements
          trulia.ContactUser.prepareLeadRecommenderBypassElements();

          // show the bypass checkbox
          $lead_recommender_bypass_container.removeClass('hideFully');
        }
      }

      // lrmBypassAjaxResponseHandler is only called after the lrmBypassAjax call is done
      lrmBypassAjax.done(lrmBypassAjaxResponseHandler);
    }
  });
</script>
