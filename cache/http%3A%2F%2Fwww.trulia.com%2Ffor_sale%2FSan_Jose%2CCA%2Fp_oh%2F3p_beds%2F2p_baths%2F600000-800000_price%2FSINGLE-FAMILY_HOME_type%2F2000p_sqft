  <script>
  trulia.ads_data = {};

  
  if (window.srpRowTextAdConfig)
  {
    trulia.ads_filters.push(function filterInlineTextLinkSlot(key, data)
    {
      if (key == 'srp_row_text_ad')
      {
        data.sizes = window.srpRowTextAdConfig.sizes;
      }
    });
  }

  
  trulia.ads_filters.push(function filterTargetRTTSlots(key, data)
  {
    
    var adSlotPattern   = /^ad_srt([0-9]{1,2})/, // 
        adSlotMatches   = null,
        flagData        = null,
        $propertyCardId = null;

    
    adSlotMatches = key.match(adSlotPattern);

    if (adSlotMatches)
    {
      
      $propertyCardId = $('#propertyAnchor_' + (adSlotMatches[1] - 1));

      
      flagData = $propertyCardId.data('rtt');

      if (flagData)
      {
        if (!data.keyvals)
        {
          data.keyvals = [];
        }

        
        data.keyvals.push("tflag=" + flagData);
      }
      else
      {
        
        return true;
      }
    }
  });

  
  trulia.ads_filters.push(function filterMortgagePrequal(key, data)
  {
    if (!trulia.isDefault() && key.indexOf('srp_mm_link') == 0)
    {
      return true;
    }
  });

</script>
  
  
