    <script type="text/javascript">
    (function () {
      //if we have a hash in our url and it looks like it might be a url,
      //let's redirect
      var hash = window.location.href.split('#');
      if (hash.length > 1 && hash[1][0] === '/') {
        window.location.href = "/" + hash[1].substr(1);
      }
    })();
  </script>



