  // RENT-2897 and as per conversation with Matt (over phone and chat):
  // 1. Show `dtl12` unconditionally on all pages except rental-community types.
  // 2. Do not show `dtl15` unconditionally on all pages except rental-community types.
  // 3. Show both `dtl12` and `dtl15` conditionally (based on `adCampaign` flag) only in rental-community types.
  trulia.ads_filters.push(function filterOutPossibleRttSlots(key, data)
  {
    var isRentalCommunity = false;
    var isRental = false;

    // check for condition 1
    if (!isRental && key == 'ad_dtl12')
    {
      // never show if for_sale pdp
      // true – do filter out
      return true;
    }
    else if (!isRentalCommunity && key == 'ad_dtl12')
    {
      // always show if not rental community
      // undefined – do not filter out
      return;
    }
    else if (!isRentalCommunity && key == 'ad_dtl15')
    {
      // never show if not rental community
      // true – do filter out
      return true;
    }
    else if (isRentalCommunity && (key == 'ad_dtl12' || key == 'ad_dtl15'))
    {
      // deal with rental-community type of the pages here
      // presence of the ad slot is controlled on the template level
      // so just check if DOM element don't exists and return true
      // to filter it out from the ad call
      if (!document.getElementById(key))
      {
        return true;
      }
    }

  });




trulia.ads_filters.push(function filterTopSpanAdABTest(key, data)
{
  if (key == 'top_span_ad' && typeof test_ads_topspan == 'string' && test_ads_topspan == 'b')
  {
    return true;
  }
});

trulia.ads_targets.push(function addTopSpanAdABTestTrackingTargeting(ad)
{
  // just to be safe
  if (!ad.setTargeting) return;

  if (typeof test_ads_topspan == 'string')
  {
    ad.setTargeting('vers', test_ads_topspan);
  }
});
</script>




